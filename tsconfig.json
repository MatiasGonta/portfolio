{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["./src/layouts/*"],
      "@i18n/*": ["./src/i18n/*"],
      "@components/*": ["./src/components/*"],
      "@models/*": ["./src/models/*"],
      "@icons/*": ["./src/icons/*"],
      "@assets/*": ["./src/assets/*"],
      "@data/*": ["./src/data/*"],
    },
    "jsxImportSource": "react"
  }
}